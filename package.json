{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build-back": "tsc -p tsconfig.back.json",
    "start-back": "nodemon",
    "pm2": "npm run build-back && node build/app.js",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.3",
    "codemirror": "^5.59.4",
    "cors": "^2.8.5",
    "darkreader": "^4.9.27",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "got": "^11.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "node-fetch": "^2.6.1",
    "qrcode.react": "^1.0.1",
    "react-codemirror2": "^7.2.1",
    "react-diff-viewer": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "umi": "^3.3.9",
    "umi-request": "^1.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "^6.0.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.8",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.3.9",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
